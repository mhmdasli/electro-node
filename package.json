  },
  "license": "MIT",
  "devDependencies": {
    "electron": "^5.0.7",
    "electron-installer-windows": "^2.0.0",
    "electron-winstaller": "^3.0.4"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.1.2"
  }
}
