{
  "name": "electro-node",
  "version": "1.0.0",
  "description": "electron-node App",
  "main": "main.js",
  "scripts": {
    "prebuild": "npm install && cd ./app && npm install",
    "start": "electron .",
    "build": "electron-packager . electro-node --platform win32 --arch x64 --out dist/",
    "setup": "electron-installer-windows --src dist/electro-node-win32-x64/ --dest dist/installers/"
  },
